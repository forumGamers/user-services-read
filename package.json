    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/nodemon": "^1.19.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
